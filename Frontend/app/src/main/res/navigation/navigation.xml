<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.tradeasy.ui.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.tradeasy.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />

        <action
            android:id="@+id/action_loginFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"

            app:destination="@id/homeFragment"

            />
        <action
            android:id="@+id/action_loginFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_loginFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.tradeasy.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_profileFragment_to_loginFragment"

            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_profileFragment_to_bidsFragment"
            app:destination="@id/bidsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_purchasesFragment"
            app:destination="@id/purchasesFragment" />
        <action
            android:id="@+id/action_profileFragment_to_pushNotificationsFragment"
            app:destination="@id/pushNotificationsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_savedProductsFragment"
            app:destination="@id/savedProductsFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.tradeasy.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_productItemFragment"
            app:destination="@id/productItemFragment" />
        <action
            android:id="@+id/action_homeFragment_to_productsByCategoryFragment"
            app:destination="@id/productsByCategoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.tradeasy.ui.editProfile.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <action
            android:id="@+id/action_editProfileFragment_to_updatePasswordFragment"
            app:destination="@id/updatePasswordFragment" />
        <action
            android:id="@+id/action_editProfileFragment_to_updateUsernameFragment"
            app:destination="@id/updateUsernameFragment" />
        <action
            android:id="@+id/action_editProfileFragment_to_updatePnFragment"
            app:destination="@id/updatePnFragment" />
        <action
            android:id="@+id/action_editProfileFragment_to_updateEmailFragment"
            app:destination="@id/updateEmailFragment" />
        <action
            android:id="@+id/action_editProfileFragment_to_cropProfilePicFragment"
            app:destination="@id/cropProfilePicFragment" />
    </fragment>
    <fragment
        android:id="@+id/updatePasswordFragment"
        android:name="com.tradeasy.ui.editProfile.updatePassword.UpdatePasswordFragment"
        android:label="fragment_update_password"
        tools:layout="@layout/fragment_update_password">
        <action
            android:id="@+id/action_updatePasswordFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.tradeasy.ui.notifications.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_notificationsFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.tradeasy.ui.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_searchDetailsFragment"
            app:destination="@id/searchDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/sellingFragment"
        android:name="com.tradeasy.ui.selling.SellingFragment"
        android:label="fragment_selling"
        tools:layout="@layout/fragment_selling" >
        <action
            android:id="@+id/action_sellingFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_sellingFragment_to_addProductFragment"
            app:destination="@id/addProductFragment" />
        <action
            android:id="@+id/action_sellingFragment_to_productItemFragment"
            app:destination="@id/productItemFragment" />
        <action
            android:id="@+id/action_sellingFragment_to_userProductItemFragment"
            app:destination="@id/userProductItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/updateUsernameFragment"
        android:name="com.tradeasy.ui.editProfile.updateUsername.UpdateUsernameFragment"
        android:label="fragment_update_username"
        tools:layout="@layout/fragment_update_username" />
    <fragment
        android:id="@+id/updatePnFragment"
        android:name="com.tradeasy.ui.editProfile.updatePn.UpdatePnFragment"
        android:label="fragment_update_pn"
        tools:layout="@layout/fragment_update_pn" />
    <fragment
        android:id="@+id/updateEmailFragment"
        android:name="com.tradeasy.ui.editProfile.updateEmail.UpdateEmailFragment"
        android:label="fragment_update_email"
        tools:layout="@layout/fragment_update_email" />
    <fragment
        android:id="@+id/addProductFragment"
        android:name="com.tradeasy.ui.selling.product.AddProductFragment"
        android:label="fragment_add_product"
        tools:layout="@layout/fragment_add_product" >
        <action
            android:id="@+id/action_addProductFragment_to_additionalInfoFragment"
            app:destination="@id/additionalInfoFragment" />
        <action
            android:id="@+id/action_addProductFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
    </fragment>
    <fragment
        android:id="@+id/placeBidFragment"
        android:name="com.tradeasy.ui.selling.bid.PlaceBidFragment"
        android:label="fragment_bid"
        tools:layout="@layout/fragment_place_bid" >
        <argument
            android:name="productId"
            app:argType="string" />
        <argument
            android:name="bidTime"
            app:argType="string" />
        <argument
            android:name="productPrice"
            app:argType="float" />
        <argument
            android:name="forBid"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/purchasesFragment"
        android:name="com.tradeasy.ui.profile.purchases.PurchasesFragment"
        android:label="fragment_purchases"
        tools:layout="@layout/fragment_purchases" />
    <fragment
        android:id="@+id/pushNotificationsFragment"
        android:name="com.tradeasy.ui.profile.pushNotifications.PushNotificationsFragment"
        android:label="fragment_push_notifications"
        tools:layout="@layout/fragment_push_notifications" />
    <fragment
        android:id="@+id/bidsFragment"
        android:name="com.tradeasy.ui.profile.bids.BidsFragment"
        android:label="fragment_bids"
        tools:layout="@layout/fragment_bids" />
    <fragment
        android:id="@+id/additionalInfoFragment"
        android:name="com.tradeasy.ui.selling.product.AdditionalInfoFragment"
        android:label="fragment_additional_info"
        tools:layout="@layout/fragment_additional_info" >
        <argument
            android:name="prodName"
            app:argType="string" />
        <argument android:name="prodDesc"
            app:argType="string" />
        <argument
            android:name="prodPrice"
            app:argType="string" />
        <action
            android:id="@+id/action_additionalInfoFragment_to_bidChoicesFragment"
            app:destination="@id/bidChoicesFragment" />
    </fragment>
    <fragment
        android:id="@+id/productItemFragment"
        android:name="com.tradeasy.ui.selling.product.item.ProductItemFragment"
        android:label="fragment_product_item"
        tools:layout="@layout/fragment_product_item" >
        <argument
            android:name="productName"
            app:argType="string" />
        <argument
            android:name="productDesc"
            app:argType="string" />
        <argument
            android:name="productCategory"
            app:argType="string" />
        <argument
            android:name="productPrice"
            app:argType="float" />
        <argument
            android:name="bidTime"
            app:argType="string" />
        <argument
            android:name="productQuantity"
            app:argType="integer" />
        <argument
            android:name="addedDate"
            app:argType="string" />
        <argument
            android:name="forBid"
            app:argType="boolean" />
        <argument
            android:name="bidEndDate"
            app:argType="string" />
        <argument
            android:name="productId"
            app:argType="string" />
        <argument
            android:name="username"
            app:argType="string" />
        <argument
            android:name="userPhoneNumber"
            app:argType="string" />
        <argument
            android:name="userProfilePicture"
            app:argType="string" />
        <action
            android:id="@+id/action_productItemFragment_to_placeBidFragment"
            app:destination="@id/placeBidFragment" />
        <argument android:name="image"
            app:argType="string[]" />
    </fragment>
    <fragment
        android:id="@+id/savedProductsFragment"
        android:name="com.tradeasy.ui.profile.saved.SavedProductsFragment"
        android:label="fragment_saved_products"
        tools:layout="@layout/fragment_saved_products" >
        <action
            android:id="@+id/action_savedProductsFragment_to_productItemFragment"
            app:destination="@id/productItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchDetailsFragment"
        android:name="com.tradeasy.ui.search.details.SearchDetailsFragment"
        android:label="fragment_search_details"
        tools:layout="@layout/fragment_search_details" >
        <argument
            android:name="searchInput"
            app:argType="string"
            android:defaultValue="@null"
            app:nullable="true" />
        <action
            android:id="@+id/action_searchDetailsFragment_to_productItemFragment"
            app:destination="@id/productItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.tradeasy.ui.login.forgotPassword.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPasswordFragment_to_verifyOtpFragment"
            app:destination="@id/verifyOtpFragment" />
    </fragment>
    <fragment
        android:id="@+id/verifyOtpFragment"
        android:name="com.tradeasy.ui.login.forgotPassword.verifyOtp.VerifyOtpFragment"
        android:label="fragment_verify_otp"
        tools:layout="@layout/fragment_verify_otp" >
        <action
            android:id="@+id/action_verifyOtpFragment_to_resetPassFragment"
            app:destination="@id/resetPassFragment" />
        <argument
            android:name="email"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/resetPassFragment"
        android:name="com.tradeasy.ui.login.forgotPassword.resetPass.ResetPassFragment"
        android:label="fragment_reset_pass"
        tools:layout="@layout/fragment_reset_pass" >
        <argument
            android:name="email"
            app:argType="string" />
        <argument
            android:name="otp"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/cropProfilePicFragment"
        android:name="com.tradeasy.ui.editProfile.updateProfilePic.CropProfilePicFragment"
        android:label="fragment_crop_profile_pic"
        tools:layout="@layout/fragment_crop_profile_pic" >
        <argument android:name="profilePic" />
        <argument android:name="file" />
    </fragment>
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.tradeasy.ui.selling.product.categories.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoriesFragment_to_addProductFragment"
            app:destination="@id/addProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/bidChoicesFragment"
        android:name="com.tradeasy.ui.selling.product.bidChoices.BidChoicesFragment"
        android:label="fragment_bid_choices"
        tools:layout="@layout/fragment_bid_choices" >
        <action
            android:id="@+id/action_bidChoicesFragment_to_additionalInfoFragment"
            app:destination="@id/additionalInfoFragment" >
            <argument
                android:name="prodName"
                />
            <argument
                android:name="prodDesc"
                />
            <argument
                android:name="prodCategory"
                />
            <argument
                android:name="prodPrice"
                />
            <argument
                android:name="bidDuration"
                />
        </action>
    </fragment>
    <fragment
        android:id="@+id/productsByCategoryFragment"
        android:name="com.tradeasy.ui.home.productsByCategory.ProductsByCategoryFragment"
        android:label="fragment_products_by_category"
        tools:layout="@layout/fragment_products_by_category" >
        <argument
            android:name="category"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/userProductItemFragment"
        android:name="com.tradeasy.ui.selling.userSelling.UserProductItemFragment"
        android:label="fragment_user_product_item"
        tools:layout="@layout/fragment_user_product_item" >
        <argument
            android:name="userProdName"
            app:argType="string" />
        <argument
            android:name="userProdImg"
            app:argType="string[]" />
        <argument
            android:name="userProdPrice"
            app:argType="float" />
        <argument
            android:name="selling"
            app:argType="boolean" />
        <argument
            android:name="userProdId"
            app:argType="string" />
    </fragment>

</navigation>